G_2 <- function(dane) {
y_xo <- rowSums(dane)
y_ox <- colSums(dane)
n <- sum(dane)
r <-nrow(dane)
c <- ncol(dane)
x <- 1
for (i in 1:r) {
for (j in 1:c) {
y_ij <- dane[i,j]
x <- ( ( y_xo[i]*y_ox[j] ) / (y_ij*n) )^y_ij * x
}
}
g_2 <- -2 * log(x)
return( 1-pchisq(g_2, (c-1)*(r-1)) )
}
G_2(dane)
